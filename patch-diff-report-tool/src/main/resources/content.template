				<div class="section">
					<h3 th:text=${filename}>FileName</h3>
					<tr class="b">
					<table border="0" class="bodyTable">
					        <th></th>
							<th>Severity</th>
							<th>Rule</th>
							<th>Message</th>
							<th>Line</th>
							<th>Col</th>
						</tr>
						<th:block th:each="record : ${records}" th:object = "${anchor.increment()}">
							<th:block th:switch="${record.index}">
								<tr th:case = "1" class="b">
								<tr th:case = "*" class="a">
							</th:block>
								<td><a th:name="${anchor.getAnchor()}" th:href = "@{#} + ${anchor.getAnchor()}" th:text = @{#}> Anchor </a></td>
								<td th:text = ${record.severity}> Severity </td>
								<td th:text = ${record.simpleCuttedSourceName}> Rule </td>
								<td th:utext = ${record.messageHtml}> Message </td>
								<th:block th:switch="${record.xref}">
									<td th:case = "null" th:text = ${record.line}> Line </td>
									<td th:case = "*"><a th:href = "${record.xref} + @{#L} + ${record.line}" th:text = ${record.line}> Line </a></td>
								</th:block>
								<th:block th:switch="${record.column}">
									<td th:case = "-1"> </td>
									<td th:case = "*" th:text=${record.column}> column </td>
								</th:block>
							</tr>
						</th:block>
					</table>
				</div>
